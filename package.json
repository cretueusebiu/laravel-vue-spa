    "eslint-plugin-vue": "^7.1.0",
    "laravel-mix": "^5.0.7",
    "laravel-mix-merge-manifest": "^0.1.2",
    "laravel-mix-versionhash": "^1.1.7",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.28.0",
    "sass-loader": "^10.0.4",
    "vue-template-compiler": "^2.6.12",
